<h1>Album</h1>

<p>
  <strong>Title:</strong>
  <%= @album.title %>
</p>
<p>
  <strong>Upload Date:</strong>
  <%= @album.upload_date %>
</p>
<p>
  <strong>Modify Date:</strong>
  <%= @album.modify_date %>
</p>
<p>
  <strong>Status:</strong>
  <%= @album.status %>
</p>
<p>
  <strong>Order:</strong>
  <%= @album.order_no %>
</p>

<p>
  <%= link_to "Edit", edit_admin_album_path(@album) %> |
  <%= link_to "Destroy", [:admin,@album], :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", admin_albums_path %>
</p>
